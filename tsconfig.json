{
    "ts-node": {
        "transpileOnly": true,
        "compilerOptions": {
            "module": "commonjs",
        }
    },
    "compilerOptions": {
        "outDir": "./dist",
        "module": "esnext",
        "target": "esnext",
        "lib": ["esnext"],
        "alwaysStrict": true,
        "strict": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "types": ["@cloudflare/workers-types", "@types/jest", "@types/service-worker-mock"]
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist", "test"]
}
